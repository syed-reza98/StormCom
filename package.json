    "@radix-ui/react-toast": "^1.2.0",
    "@react-email/components": "^0.5.7",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@upstash/ratelimit": "^2.0.0",
    "@upstash/redis": "^1.34.0",
